{
  "name": "obs-tools-widget",
  "version": "1.0.4",
  "description": "",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start": "turbo run start",
    "build": "turbo run build --force --no-cache",
    "dev": "turbo run dev --parallel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luckydye/e2e-dashboard.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luckydye/e2e-dashboard/issues"
  },
  "homepage": "https://github.com/luckydye/e2e-dashboard#readme",
  "dependencies": {
    "turbo": "^1.0.28"
  },
  "turbo": {
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "dist/**"
        ]
      },
      "dev": {
        "cache": false
      },
      "start": {
        "cache": false
      }
    }
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  }
}
